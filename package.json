{
  "name": "pokemon-list",
  "homepage": "https://marcelorl.github.io/pokemon-list/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "axios-cache-adapter": "^2.0.0",
    "bootstrap": "^4.0.0",
    "gh-pages": "^2.0.1",
    "history": "^4.7.2",
    "localforage": "^1.7.1",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-app-rewired": "^2.1.0",
    "react-dom": "^16.3.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^2.1.5",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.3.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "husky": "^4.0.6",
    "jest-enzyme": "^7.0.1",
    "jest-localstorage-mock": "^2.2.0",
    "lint-staged": "^8.1.3",
    "react-cosmos": "^4.7.0",
    "react-cosmos-redux-proxy": "^4.6.1",
    "react-cosmos-router-proxy": "^4.6.1",
    "react-cosmos-test": "^4.7.0",
    "react-test-renderer": "^16.3.2",
    "server": "^1.0.18",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage",
    "lint": "standard \"src/**/*.js\" --fix --verbose | snazzy",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "cosmos": "cosmos",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix --verbose | snazzy",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!<rootDir>/node_modules/",
      "!src/*.{js}",
      "!src/cosmos.proxies.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jest",
      "describe",
      "it",
      "xit",
      "expect",
      "beforeEach"
    ]
  }
}
